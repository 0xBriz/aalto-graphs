{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/graphs/src",
  "projects": {
    "providers": {
      "type": "library",
      "root": "libs/providers",
      "entryFile": "index",
      "sourceRoot": "libs/providers/src",
      "compilerOptions": {
        "tsConfigPath": "libs/providers/tsconfig.lib.json"
      }
    },
    "graphs": {
      "type": "application",
      "root": "apps/graphs",
      "entryFile": "main",
      "sourceRoot": "apps/graphs/src",
      "compilerOptions": {
        "tsConfigPath": "apps/graphs/tsconfig.app.json"
      }
    },
    "dex": {
      "type": "application",
      "root": "apps/dex",
      "entryFile": "main",
      "sourceRoot": "apps/dex/src",
      "compilerOptions": {
        "tsConfigPath": "apps/dex/tsconfig.app.json"
      }
    },
    "indexer": {
      "type": "library",
      "root": "libs/indexer",
      "entryFile": "index",
      "sourceRoot": "libs/indexer/src",
      "compilerOptions": {
        "tsConfigPath": "libs/indexer/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "graphql": {
      "type": "library",
      "root": "libs/graphql",
      "entryFile": "index",
      "sourceRoot": "libs/graphql/src",
      "compilerOptions": {
        "tsConfigPath": "libs/graphql/tsconfig.lib.json"
      }
    },
    "data": {
      "type": "library",
      "root": "libs/data",
      "entryFile": "index",
      "sourceRoot": "libs/data/src",
      "compilerOptions": {
        "tsConfigPath": "libs/data/tsconfig.lib.json"
      }
    }
  },
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/graphs/tsconfig.app.json",
    "plugins": [
      "@nestjs/graphql"
    ]
  },
  "monorepo": true,
  "root": "apps/graphs"
}